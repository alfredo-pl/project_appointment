<p id="notice"><%= notice %></p>

<h1>Branchoffices</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Code branchoffice</th>
      <th>City</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @branchoffices.each do |branchoffice| %>
      <tr>
        <td><%= branchoffice.business_id %></td>
        <td><%= branchoffice.code_branchoffice %></td>
        <td><%= branchoffice.city %></td>
        <td><%= branchoffice.address %></td>
        <td><%= link_to 'Show', business_branchoffice_path(@business, branchoffice) %></td>
        <td><%= link_to 'Edit', edit_business_branchoffice_path(@business, branchoffice) %></td>
        <td><%= link_to 'Destroy', business_branchoffice_path(@business, branchoffice), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Branchoffice', new_business_branchoffice_path %>
